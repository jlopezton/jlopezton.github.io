@MISC{RePEc:boc:bocode:s458687,
title = {POVCALNET: Stata module to access World Bank Global Poverty and Inequality measures},
author = {Castañeda, Andrés and Lakner, Christoph and Prydz, Espen Beer and Lopez, Jorge Soler and Wu, Ruoxuan and Zhao, Qinghua},
year = {2015},
abstract = {The povcalnet commands allows Stata users to compute poverty and inequality indicators for more than 160 countries and regions in the World Bank's database of household surveys. It has the same functionality as the PovcalNet website. PovcalNet is a computational tool that allows users to estimate poverty rates for regions, sets of countries or individual countries, over time and at any poverty line.},
keywords = {poverty; inequality; data management; PolCalNet},
url = {https://EconPapers.repec.org/RePEc:boc:bocode:s458687}
}